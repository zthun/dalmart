{
  "extends": "./.config/.tsconfig.json",
  "compilerOptions": {
    "baseUrl": "packages",
    "target": "esnext",
    "paths": {
      "@zthun/dalmart-db": ["dalmart-db/src/index"],
      "@zthun/dalmart-fs": ["dalmart-fs/src/index"],
      "@zthun/dalmart-forage": ["dalmart-forage/src/index"],
      "@zthun/dalmart-memory": ["dalmart-memory/src/index"],
      "@zthun/dalmart-mongo": ["dalmart-mongo/src/index"],
      "@zthun/dalmart-redis": ["dalmart-redis/src/index"],
      "@zthun/dalmart-storage": ["dalmart-storage/src/index"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "packages/dalmart-memory/src/database-server/database-server-document.mts",
    "packages/dalmart-memory/src/database-server/database-server-memory.mts",
    "packages/dalmart-memory/src/database-server/database-server.mts",
    "packages/dalmart-mongo/src/database-mongo/database-mongo.mts",
    "packages/dalmart-mongo/src/convert/to-filter.mts",
    "packages/dalmart-mongo/src/convert/to-sort.mts",
    "packages/dalmart-redis/src/database-redis/database-redis.mts",
    "packages/dalmart-storage/src/database-storage/database-storage.mts"
  ]
}
